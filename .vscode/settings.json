{
  "python.defaultInterpreterPath": "${workspaceFolder}/../eecs545-venv/bin/python",
  "terminal.integrated.cwd": "${workspaceFolder}",
  "autopep8.args": [
    "--max-line-length=120"
  ]
  "todo-tree.general.tags": [
    "TODO",
    "FIXME",
    "CUSTOM",
    "PATCH",
    "MOD"
  ],
  "todo-tree.highlights.enabled": true,
  "todo-tree.highlights.useColourScheme": false,
  "todo-tree.highlights.customHighlight": {
    "CUSTOM": {
      "foreground": "#3cff00",
      "icon": "star",
      "iconColour": "#007acc",
      "type": "tag",
      "gutterIcon": true
    },
    "TODO": {
      "foreground": "#003cff",
      "icon": "tools",
      "iconColour": "#007acc",
      "type": "tag",
      "gutterIcon": true
    },
    "PATCH": {
      "foreground": "#ffffff",
      "icon": "tools",
      "iconColour": "#d32f2f",
      "type": "tag"
    },
    "MOD": {
      "foreground": "#000000",
      "icon": "diff-modified",
      "iconColour": "#ffeb3b",
      "type": "tag"
    }
  },
  "todo-tree.regex.regex": "(//|#|\"\"\"|'''|/\\*)\\s*($TAGS)"
}